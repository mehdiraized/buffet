    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "invariant": "^2.2.4",
    "moment": "^2.24.0",
    "rc-input-number": "^4.5.0",
    "react-dates": "^21.1.0",
    "react-moment-proptypes": "^1.7.0"
  },
  "devDependencies": {
    "enzyme": "^3.10.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
