{
  "name": "@buffetjs/core",
  "version": "1.1.1",
  "description": "Buffetjs Core Components",
  "main": "dist/index.js",
  "module": "src/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
